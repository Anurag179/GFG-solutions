class Solution 
{
    //Function to reverse words in a given string.
    String reverseWords(String s)
    {
        // code here
        String s1[]=s.split("\\.");
        String s2="";
        for(int i=s1.length-1;i>=0;i--){
            if(i!=0){
                s2=s2+s1[i]+".";
                
            }
            else{
                s2=s2+s1[i];
                //System.out.println(s2);
            }
        }
        
     return s2;
    }
}
